<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:wh="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_closed.GoActivity">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@mipmap/qiaodan"
        android:scaleType="fitXY"
        />
    <LinearLayout
        android:layout_marginTop="10dp"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="350dp"
        >
        <com.jeq.myapplication.view.progressWH
            android:id="@+id/progress_WH"
            android:layout_width="50dp"
            android:layout_height="50dp"
            wh:circleColor="@color/in"
            wh:radius="15dp"
            wh:ringBgColor="@color/ringbg"
            wh:ringColor="@color/ring"
            wh:strokeWidth="3dp"
            wh:textColor="@color/text"

            />
    </LinearLayout>


</RelativeLayout>